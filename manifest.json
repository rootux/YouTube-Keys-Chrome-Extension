{
  "name": "Tube Keys",
  "version": "0.1",
  "description": "The missing keyboard shortcuts for YouTube",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
        "js": ["jquery.min.js", "tubeKeys.js"]
    }
  ],
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "http://*.youtube.com/*",
    "https://*.youtube.com/*"
  ]
}
